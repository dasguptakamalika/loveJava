class Box {
    int value;
    Box(int v) { value = v; }
}

public class SeparateExample { 
    public static void main(String[] args) { 
        Box a = new Box(7);   // a → Box with value = 7 
        Box b = new Box(7);   // b → DIFFERENT Box with value = 7 
        b.value = 200;        // change only b's object

        System.out.println(a.value); // 7 
        System.out.println(b.value); // 200
    }
}
