class Box { int value; Box(int v){ value = v; } }

public class S7 {
    public static void main(String[] args) {
        Box a = new Box(5);
        Box b = new Box(5);
        b.value = 100;
        System.out.println(a.value); // 5   (different objects)
        System.out.println(b.value); // 100
    }
}
